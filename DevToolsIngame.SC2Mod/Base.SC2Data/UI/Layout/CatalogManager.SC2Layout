	<Frame type="Control" name="CatalogManagerAllowTech" template="CatalogManager/CatalogManager">
		<Frame type="Button" name="AllowButton" template="DevTools_Templates/ButtonTemplate">
			<Anchor side="Bottom" relative="$parent/AnchorContainerFrame" pos="Max" offset="0"/>
			<Anchor side="Left" relative="$parent/AnchorContainerFrame" pos="Min" offset="0"/>
